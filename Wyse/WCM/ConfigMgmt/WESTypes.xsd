  <xs:simpleType name="USBAccessType">
    <xs:restriction base="xs:string">
      <xs:enumeration id="Disabled" value="Disabled"/>
      <xs:enumeration id="Enable_Read" value="Enable Read"/>
      <xs:enumeration id="Enable_Write" value="Enable Write"/>
    </xs:restriction>
  </xs:simpleType>


  <!--Wireless configuration-->
  <xs:complexType name="WirelessSettingsType">
    <xs:all>
      <xs:element name="currentProfile" type="xs:string"  minOccurs="0"/>
      <xs:element name="swRadioState" type="swRadioStateType"  minOccurs="0"/>
      <xs:element name="wlanProfileSet" type="wlanProfileSetType"  minOccurs="0"/>
    </xs:all>
  </xs:complexType>

  <xs:simpleType name="swRadioStateType">
