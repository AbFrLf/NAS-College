      <xs:element name="repeatDelayMs" type="repeatDelayMsType" minOccurs="0"/>
      <xs:element name="repeatRate" type="repeatRateType" minOccurs="0"/>
      <xs:element name="keyboardPref" type="xs:boolean" minOccurs="0"/>
      <xs:element name="menuAccess" type="xs:boolean" minOccurs="0"/>
      <xs:element name="cursorBlinkRate" type="cursorBlinkRateType" minOccurs="0"/>
