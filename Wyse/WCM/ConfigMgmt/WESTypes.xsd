      <xs:enumeration id="Display_800_x_600" value="800x600"/>
      <xs:enumeration id="Display_1024_x_600" value="1024x600"/>
      <xs:enumeration id="Display_1024_x_768" value="1024x768"/>
      <xs:enumeration id="Display_1152_x_684" value="1152x684"/>
      <xs:enumeration id="Display_1152_x_864" value="1152x864"/>
      <xs:enumeration id="Display_1280_x_720" value="1280x720"/>
      <xs:enumeration id="Display_1280_x_768" value="1280x768"/>
      <xs:enumeration id="Display_1280_x_800" value="1280x800"/>
      <xs:enumeration id="Display_1280_x_1024" value="1280x1024"/>
      <xs:enumeration id="Display_1366_x_768" value="1366x768"/>
      <xs:enumeration id="Display_1400_x_1050" value="1400x1050"/>
      <xs:enumeration id="Display_1440_x_900" value="1440x900"/>
      <xs:enumeration id="Display_1600_x_1200" value="1600x1200"/>
      <xs:enumeration id="Display_1680_x_1050" value="1680x1050"/>
      <xs:enumeration id="Display_1776_x_1000" value="1776x1000"/>
      <xs:enumeration id="Display_1920_x_1080" value="1920x1080"/>
      <xs:enumeration id="Display_1920_x_1200" value="1920x1200"/>
      <xs:enumeration id="Display_1920_x_1440" value="1920x1440"/>
