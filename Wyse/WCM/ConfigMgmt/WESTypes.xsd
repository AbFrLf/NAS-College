  <!--
		The displayOrientation tag specifies the orientation at which images should be presented. 
		Default:    The display orientation is the natural orientation of the display device.
		90 degrees: The display orientation is rotated 90 degrees (measured clockwise) from DMDO_DEFAULT.
		180 degrees:The display orientation is rotated 180 degrees (measured clockwise) from DMDO_DEFAULT.
		270 degrees:The display orientation is rotated 270 degrees (measured clockwise) from DMDO_DEFAULT.
	-->

  <xs:simpleType name="displayOrientationType">
    <xs:restriction base="xs:string">
      <xs:enumeration id="Landscape" value="0"/>
