  </xs:complexType>
  <!--    
	<xs:simpleType name="bitmapFileType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z_-][a-zA-Z0-9_.-]*"/>
			<xs:minLength value="4"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
-->
  <xs:complexType name="displayParamsType">
    <xs:sequence>
      <xs:element name="multipleDisplays" type="multipleDisplaysType" minOccurs="0"/>
      <xs:element name="manual" type="displayManualParamsType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="multipleDisplaysType">
    <xs:restriction base="xs:string">
      <xs:enumeration id="Single_Monitor" value="Single"/>
      <xs:enumeration id="Extend_these_displays" value="Extend"/>
      <xs:enumeration id="Duplicate_these_displays" value="Clone"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:group name="displayConfigurationGroup">
    <xs:sequence>
      <xs:element name="displayResolution" type="displayResolutionType" minOccurs="0"/>
      <xs:element name="displayRefreshRate" type="displayRefreshRateType" minOccurs="0"/>
      <xs:element name="colorDepth" type="colorDepthType" minOccurs="0"/>
    </xs:sequence>
  </xs:group>

  <xs:complexType name="displayManualParamsType">
    <xs:sequence>
      <xs:element name="displayIdentifier" type="xs:string" minOccurs="0"/>
      <xs:element name="primary" type="xs:boolean" minOccurs="0"/>
      <xs:choice>
        <xs:element name="defaultDisplayConfiguration" type="defaultDisplayConfigType" minOccurs="0"/>
        <xs:group ref="displayConfigurationGroup" minOccurs="0"/>
      </xs:choice>
      <xs:element name="displayOrientation" type="displayOrientationType" minOccurs="0"/>
      <xs:element name="displayPosition" type="displayPositionType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="defaultDisplayConfigType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
