    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="displayDesktopBackgroundType">
    <xs:all>
      <xs:element name="file" type="bitmapFileType" minOccurs="0"/>
      <xs:element name="layout" type="layoutType" minOccurs="0"/>
