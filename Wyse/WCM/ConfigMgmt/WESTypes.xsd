      <xs:element name="repeatDelayMs" type="repeatDelayMsType"/>
      <xs:element name="repeatRate" type="repeatRateType"/>
    </xs:all>
  </xs:complexType>

  <!-- Keyboard repeat delay configuration.  -->
  <!-- short is value of 0(250 ms) -->
  <!-- mediumShort is value of 1(500 ms) -->
