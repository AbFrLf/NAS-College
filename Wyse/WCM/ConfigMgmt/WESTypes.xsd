      <xs:element name="mouseSpeed" type="mouseSpeedType" minOccurs="0"/>
      <xs:element name="mouseSwap" type="xs:boolean" minOccurs="0"/>
      <xs:element name="pointerTrail" type="pointerTrailType" minOccurs="0"/>
      <xs:element name="scrollLines" type="mouseScrollType" minOccurs="0"/>
      <xs:element name="doubleClickSpeed" type="mouseDoubelClickType" minOccurs="0"/>
      <xs:element name="snapPointer" type="xs:boolean" minOccurs="0"/>
